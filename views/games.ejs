<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Games Collection</title>
    </head>
    <body>
        <%- include('header') %>
        <h1>Games</h1>
        <ul>
            <% games.forEach((game) => { %>
                <li>
                    <img src="<%= game.img_url %>" alt="<%= game.title %>">
                    <p><%= game.title %></p>
                </li>
            <% }); %>
        </ul>
    </body>
</html>